{
  "expo": {
    "name": "Recipease",
    "slug": "recipease",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/wowCooking.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/wowCooking.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/wowCooking.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.chilichunk.recipease"
    },
    "web": {
      "favicon": "./assets/wowCooking.png"
    },
    "plugins": [
      "expo-font",
      "./plugins/withDynamicAppIconFix",
      [
        "expo-dynamic-app-icon",
        {
          "wow_cooking": {
            "image": "./assets/wowCooking.png",
            "prerendered": true
          },
          "modern": {
            "image": "./assets/icon.png",
            "prerendered": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c896b920-637f-4903-98c0-9544033e7d78"
      }
    }
  }
}
