{
  "expo": {
    "name": "Recipease",
    "slug": "recipease",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/wowCooking.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/wowCooking.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/wowCooking.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/wowCooking.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-dynamic-app-icon",
        {
          "wowCooking": {
            "image": "./assets/wowCooking.png",
            "prerendered": true
          },
          "modern": {
            "image": "./assets/icon.png",
            "prerendered": true
          }
        }
      ]
    ]
  }
}
